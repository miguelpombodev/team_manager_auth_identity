{
  "ConnectionStrings": {
    "TeamManager": "Host=localhost;Port=5432;Database=TeamManager;Username=postgres;Password=docker123",
    "RedisConnection": "localhost:6379,password=TeamManagerRedis123!"
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId"
    ]
  },
  "Jwt": {
    "Issuer": "TeamMemberIssuer",
    "Audience": "TeamMemberAudience",
    "ExpirationInMinutes": 5,
    "Secret": "super-secret-hidden-value-for-my-tokens"
  },
  "AllowedHosts": "*"
}
