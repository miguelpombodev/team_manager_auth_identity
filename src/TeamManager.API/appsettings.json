{
  "ConnectionStrings": {
    "TeamManager": "Host=localhost;Port=5432;Database=TeamManager;Username=postgres;Password=docker123"
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
